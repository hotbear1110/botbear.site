<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Commands</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <header class="header">
        <div class="header__image">botbear1110</div>
    </header>

    <h1>Commands</h1>
    <div class="wrapper">
        <div class="commands">
    <% commands.forEach(function(command) { %>

        <div class="commands__row">
        <h2>Command: <%- command.Name %></h2>
        <h3>Category: <%- command.Category%></h3>
        <h3>Permission: <%- command.Perm%></h3>
        <p>Description: <%- command.Command%></p>
    </div>

    <% }) %>
</div>
</div>

</body>
</html>